{
  "name": "webrtc-analyzer",
  "version": "0.1.3",
  "description": "WebRTC Connection Analyzer",
  "main": "./build/webrtc-analyzer.js",
  "scripts": {
    "browserify": "webpack --config webpack.window.js",
    "dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "prepublish": "yarn run build",
    "prettify": "prettier --config .prettierrc --write \"src/**/!(Lesshat).{less,js}\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": "https://github.com/DoclerLabs/webrtc-analyzer",
  "author": "Gabriel Micko <micsko@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node5": "^12.0.1",
    "babel-preset-stage-3": "^6.24.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "css-loader": "^0.28.11",
    "jest": "^23.4.2",
    "jsdom": "^11.12.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "uglifyjs-webpack-plugin": "^1.2.5"
  },
  "jest": {
